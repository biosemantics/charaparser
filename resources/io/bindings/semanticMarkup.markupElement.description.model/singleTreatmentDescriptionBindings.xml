<?xml version="1.0"?>
<xml-bindings
    xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/oxm"
    package-name="semanticMarkup.markupElement.description.model">
    <java-types>
        <java-type name="AbstractDescriptionsFile">
            <java-attributes>
                <xml-transient java-attribute="file"/>
            </java-attributes>
        </java-type>
   		<java-type name="DescriptionsFile">
			<xml-root-element name="treatment"/>
			<xml-type prop-order="meta treatmentRoot treatmentRoots" />
            <java-attributes>
                <xml-element java-attribute="meta" xml-path="meta" type="semanticMarkup.markupElement.description.model.Meta"/>
				<xml-element java-attribute="treatmentRoot" xml-path="." type="semanticMarkup.markupElement.description.model.TreatmentRoot" />
				<xml-transient java-attribute="treatmentRoots"/>
            </java-attributes>
        </java-type>
    </java-types>
</xml-bindings>


<?xml version="1.0"?>
<xml-bindings
    xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/oxm"
    package-name="edu.arizona.biosemantics.semanticmarkup.markupelement.description.model">
    <java-types>
   		<java-type name="DescriptionsFile">
			<xml-root-element name="treatments"/>
            <java-attributes>
                <xml-element java-attribute="meta" xml-path="meta" type="edu.arizona.biosemantics.semanticmarkup.markupelement.description.model.Meta"/>
				<xml-element java-attribute="taxonIdentifications" xml-path="taxon_identification" type="edu.arizona.biosemantics.semanticmarkup.markupelement.description.model.TaxonIdentification" container-type="java.util.List"/>        
				<xml-element java-attribute="treatmentRoots" xml-path="treatment" type="edu.arizona.biosemantics.semanticmarkup.markupelement.description.model.TreatmentRoot" container-type="java.util.List" />
            </java-attributes>
        </java-type>
    </java-types>
</xml-bindings>


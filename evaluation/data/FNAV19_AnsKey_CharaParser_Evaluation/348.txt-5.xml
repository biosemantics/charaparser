<?xml version="1.0" encoding="UTF-8"?>
<description xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:noNamespaceSchemaLocation="../../../../../workspace/characterStatements/characterAnnotationSchema.xsd">
    <statement id="s1">
        <structure id="o1" name="blade" constraint="leaf">
            <character name="shape" value="orbiculate"/>
            <character name="shape" value="ovate"/>
            <character name="shape" value="oblong"/>
            <character name="shape" char_type="range_value" from="lanceolate" to="oblanceolate"/>
            <character name="shape" value="lanceolate"/>
            <character name="shape" value="oblanceolate"/>
            <character name="shape" value="linear"/>
            <character name="shape" value="filiform"/>
        </structure>
        <structure id="o2" name="margin">
            <character name="shape" value="entire"/>
            <character name="shape" char_type="range_value" from="denticulate" to="lobed"/>
            <character name="shape" value="denticulate"/>
            <character name="shape" value="lobed" modifier="pinnately"/>
        </structure>
        <text>blades orbiculate , ovate , oblong , or lanceolate to oblanceolate , linear , or filiform , 
            margins entire or denticulate to pinnately lobed .</text>
    </statement>
</description>

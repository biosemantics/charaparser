<?xml version="1.0" encoding="UTF-8"?>
<description xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:noNamespaceSchemaLocation="../../../../../workspace/characterStatements/characterAnnotationSchema.xsd">
    <statement id="s1">
        <structure id="o1" name="interarea" constraint="ventral">
            <character name="arrangement" value="disposed" modifier="variably"/>
        </structure>
        <structure id="o2" name="perideltidium">
            <character name="development" value="developed" modifier="usually;well"/>
        </structure>
        <structure id="o3" name="pseudodeltidium">
            <character name="shape" value="convex" modifier="broadly"/>
            <character name="shape" value="flat" constraint="with monticulus" constraintid="o4"/>
        </structure>
        <structure id="o4" name="monticulus">
            <character name="size" value="narrow"/>
        </structure>
   
        <text>ventral interarea variably disposed , perideltidium usually well developed ,
            pseudodeltidium broadly convex or flat with narrow monticulus ;</text>
    </statement>
</description>

        <structure id="o5" name="slope" constraint="posterior"></structure>
        <relation id="r2" from="o2" name="bearing" negation="false" to="o3"/>
        <relation id="r3" from="o2" name="bearing" negation="false" to="o4"/>
        <relation id="r4" from="o3" name="on" negation="false" to="o5"/>
        <relation id="r5" from="o4" name="on" negation="false" to="o5"/>

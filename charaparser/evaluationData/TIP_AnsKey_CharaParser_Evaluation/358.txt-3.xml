<?xml version="1.0" encoding="UTF-8"?>
<description xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:noNamespaceSchemaLocation="../../../../../workspace/characterStatements/characterAnnotationSchema.xsd">
    <statement id="s1">
        <structure id="o1" name="plate" constraint="dental">
            <character name="size" value="short"/>
            <character name="arrangement" value="parallel"/>
            <character name="arrangement" value="divergent" modifier="narrowly"/>
        </structure>
        <structure id="o2" name="field" constraint="ventral muscle">
            <character name="shape" value="cordate"/>
        </structure>
        <structure id="o3" name="scar" constraint="diductor"></structure>
        <relation id="r1" from="o2" name="with" negation="false" to="o3"/>
        <structure id="o4" name="track" constraint="median adductor"></structure>
        <relation id="r2" from="o3" name="enclosing" negation="true" to="o4"/>
        <structure id="o5" name="groove">
            <character name="depth" value="shallow"/>
        </structure>
        <relation id="r3" from="o4" name="divided by" negation="false" to="o5"/>
        <text>dental plates short , parallel or narrowly divergent , ventral muscle field cordate with diductor scars
            not enclosing median adductor tracks that are divided by shallow groove ;</text>
    </statement>
</description>

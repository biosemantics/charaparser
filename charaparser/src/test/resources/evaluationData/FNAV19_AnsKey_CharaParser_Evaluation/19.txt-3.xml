<?xml version="1.0" encoding="UTF-8"?>
<description xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:noNamespaceSchemaLocation="../../../../../workspace/characterStatements/characterAnnotationSchema.xsd">
    <statement id="s1">
        <structure id="o1" name="blade" constraint="leaf">
            <character name="shape" value="elliptic"/>
            <character name="shape" char_type="range_value" from="elliptic-obovate" to="obovate"/>
            <character name="shape" value="elliptic-obovate"/>
            <character name="shape" value="obovate"/>
            <character name="shape" value="obovate-elliptic"/>
            <character name="shape" value="ovate"/>
            <character name="shape" value="sublyrate"/>
        </structure>
        <structure id="o2" name="base" constraint="leaf">
            <character name="shape" value="cuneate"/>
        </structure>
        <structure id="o3" name="margin" constraint="leaf">
            <character name="shape" value="entire"/>
            <character name="shape" value="denticulate"/>
            <character name="shape" value="serrulate"/>
            <character name="shape"  char_type="range_value" from="dentate" to="lobed"/>
            <character name="shape" value="dentate"/>
            <character name="shape" value="lobed"/>
        </structure>
        <structure id="o4" name="face" constraint="abaxial" />
        <structure id="o5" name="face" constraint="adaxial">
            <character name="pubescence" value="glabrous"/>
            <character name="pubescence" value="glabrescent"/>
        </structure>
        <structure id="o6" name="wool">
            <character name="texture" value="dense"/>
        </structure>
        <relation id="r1" from="o4" name="covered with" negation="false" to="o6"/>
        <text>blades elliptic or elliptic_obovate to obovate , obovate_elliptic , ovate , or sublyrate , 
            bases cuneate , margins entire or denticulate , serrulate , or dentate to lobed , abaxial faces
            usually covered with dense wool , adaxial faces glabrous or glabrescent .</text>
    </statement>
</description>
